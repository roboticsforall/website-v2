{
  "format": "page",
  "id": "page",
  "label": "Page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "category": {
        "type": "Link",
        "config": {
          "label": "Category",
          "select": "document",
          "customtypes": ["category"]
        }
      },
      "navbar_color": {
        "type": "Select",
        "config": {
          "label": "Navbar Color",
          "placeholder": "",
          "options": ["white", "primary"],
          "default_value": "white"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "column_cards": {
              "type": "SharedSlice"
            },
            "theming": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
